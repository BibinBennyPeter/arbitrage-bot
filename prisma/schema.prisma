generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Results {
  id        Int     @id @default(autoincrement())
  dex       String
  chain     Int
  pairKey      String
  token0    String
  token1    String
  priceBuy  Float
  priceSell Float
  profit    Float
  reserve0  String
  reserve1  String
  fee       Float
  timestamp Int
}
